    <TIER ANNOTATOR="osakhsa" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@yit1988m"
        PARTICIPANT="@yit1988m" TIER_ID="transl@yit1988m">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a19" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>This Chinese accent in world pianism will grow only.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a20" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>How it all turned out?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a21" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>In all of this Asian countries</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a22" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>existed a few main f... forms of music being.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a23" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>The first is music as part of a ritual, of a religious ceremony.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>The second is music for a theatre. Theatre is very important in these countries and always was connected with music. There was no non-musical theatre itself.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Next point, there was so-called local classical music.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>It is a difficult concept,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>m...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>it is difficult to define it,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>but, in fact, there are musicians-professionals who study</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>clear established canons of performing,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>who perform</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>for emperors in palaces, in general, for elite,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>translate their knowledge to next generations, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>quite similar to European classical music.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>And the last is folklore,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>which separated from ritualistic and ceremonial music over time.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
